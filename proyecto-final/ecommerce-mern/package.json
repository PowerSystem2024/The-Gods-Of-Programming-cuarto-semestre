{
  "name": "ecommerce-mern-fullstack",
  "version": "1.0.0",
  "description": "E-commerce fullstack application with MERN stack",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "build": "npm run client:build && npm run server:build",
    "start": "npm run server:start",
    "server:dev": "cd backend && npm run dev",
    "server:start": "cd backend && npm start",
    "server:build": "cd backend && npm run build",
    "client:dev": "cd frontend && npm run dev",
    "client:build": "cd frontend && npm run build",
    "client:preview": "cd frontend && npm run preview",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "clean:all": "rm -rf node_modules && cd backend && rm -rf node_modules && cd ../frontend && rm -rf node_modules",
    "lint": "cd frontend && npm run lint",
    "lint:fix": "cd frontend && npm run lint:fix",
    "deploy:build": "npm run install:all && npm run build",
    "health:check": "curl http://localhost:5000/api/health || echo 'Backend not running'"
  },
  "keywords": [
    "ecommerce",
    "mern",
    "react",
    "nodejs",
    "express",
    "mongodb",
    "fullstack"
  ],
  "author": "Los Dioses de la ProgramaciÃ³n",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PowerSystem2024/The-Gods-Of-Programming-cuarto-semestre.git"
  },
  "bugs": {
    "url": "https://github.com/PowerSystem2024/The-Gods-Of-Programming-cuarto-semestre/issues"
  }
}