{
  "name": "ecommerce-backend",
  "version": "1.0.0",
  "description": "Backend para aplicaci√≥n de e-commerce con MERN stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "echo 'Backend build completed - No build step required for Node.js'",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --forceExit",
    "test:unit": "cross-env NODE_ENV=test jest --testPathPattern=unit --detectOpenHandles --forceExit",
    "test:integration": "cross-env NODE_ENV=test jest --testPathPattern=integration --detectOpenHandles --forceExit",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage --detectOpenHandles --forceExit",
    "serve": "npm start",
    "prod": "NODE_ENV=production node server.js",
    "clean": "rm -rf node_modules uploads/*.jpg uploads/*.png uploads/*.jpeg",
    "install:clean": "npm run clean && npm install",
    "seed": "node scripts/seed.js",
    "db:reset": "node scripts/resetdb.js"
  },
  "keywords": [
    "ecommerce",
    "node",
    "express",
    "mongodb",
    "mern"
  ],
  "author": "Tu Nombre",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-mongo": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.10",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "@types/jest": "^29.5.12",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "mongodb-memory-server": "^9.1.6"
  }
}
