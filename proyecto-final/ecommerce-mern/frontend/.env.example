# Frontend Environment Variables
# Copiar este archivo a .env y configurar las variables

# URL del backend API
VITE_API_URL=http://localhost:5000/api

# URL base para imágenes (opcional)
VITE_IMAGES_URL=http://localhost:5000/uploads

# Modo de desarrollo (opcional)
VITE_NODE_ENV=development

# Puerto del frontend (opcional - Vite usa 5173 por defecto)
VITE_PORT=3000

# Configuración de analytics (opcional)
VITE_ANALYTICS_ID=

# Configuración de pagos (opcional - para integraciones futuras)
VITE_STRIPE_PUBLIC_KEY=
VITE_PAYPAL_CLIENT_ID=